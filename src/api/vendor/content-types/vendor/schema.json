{
  "kind": "collectionType",
  "collectionName": "vendors",
  "info": {
    "singularName": "vendor",
    "pluralName": "vendors",
    "displayName": "Vendor",
    "description": "Marketplace vendor directory"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true,
      "filterable": true,
      "bulkable": true,
      "searchable": true
    },
    "content-type-builder": {
      "visible": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "required": true,
      "default": "draft",
      "enum": [
        "draft",
        "active",
        "suspended"
      ]
    },
    "description": {
      "type": "text"
    },
    "contact_email": {
      "type": "email"
    },
    "contact_phone": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "address": {
      "type": "json"
    },
    "support_notes": {
      "type": "text"
    },
    "metadata": {
      "type": "json"
    }
  }
}
